-- This generates a cylinder using Voronoi tessellation.

points = point_factory.cylinder{radius = 1, length = 10, nr = 15, nz = 100, 
                                num_ghost = 1, radial_spacing = 'log',
                                log_spacing_factor = 1.5}
                                
mesh = mesh_factory.voronoi(points)
print(mesh)
write_silo_plot(mesh, 'cylinder')
write_vtk_plot(mesh, 'cylinder')
